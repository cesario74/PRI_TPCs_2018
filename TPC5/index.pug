extends layout

block corpo
    .w3-container
        .w3-card-4
            header.w3-container.w3-light-grey
                h3 Arquivo de Música Digital
            .w3-container
                table.w3-table.w3-bordered
                    tr
                        th Tipo
                        th Titulo
                    each c
                        tr
                            td= c.tipo 
                            td
                                ul
                                    each a 
                                        li
                                            a(href='http://localhost:5000/musica/' + a.id)= a.titulo 
            footer.w3-container.w3-light-grey
                address Done by Cesário Perneta

